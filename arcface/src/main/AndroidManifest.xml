<manifest package="com.arcsoft.sdk"
xmlns:android="http://schemas.android.com/apk/res/android">
<!-- camera -->
<uses-permission android:name="android.permission.CAMERA" />
<!-- sdcard -->
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
<!-- wifi internet -->
<uses-permission android:name="android.permission.INTERNET"/>

<application
    android:allowBackup="true"
    android:supportsRtl="true">
    <activity android:name="com.arcsoft.sdk.PermissionAcitivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>

            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>
    <activity android:name="com.arcsoft.sdk.RegisterActivity">
    </activity>
    <activity android:name="com.arcsoft.sdk.DetecterActivity">
    </activity>
    <activity android:name="com.arcsoft.sdk.MainActivity">
    </activity>
</application>

</manifest>
